{
  "name": "ngx-feature-toggle",
  "private": false,
  "description": "Your module to handle with feature toggles in Angular applications easier.",
  "author": "Will Mendes <willmendesneto@gmail.com>",
  "license": "MIT",
  "version": "1.0.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "ng2",
    "ngx",
    "feature-toggle",
    "ngx-feature-toggle"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/willmendesneto/ngx-feature-toggle.git"
  },
  "bugs": {
    "url": "https://github.com/willmendesneto/ngx-feature-toggle/issues"
  },
  "homepage": "https://github.com/willmendesneto/ngx-feature-toggle#readme",
  "scripts": {
    "bundle-report": "npm run build-report && source-map-explorer dist/bundles/ngx-feature-toggle.umd.js",
    "lite-server": "lite-server -c demo/bs-config.json",
    "demo-serve": "npm run build-package-link && npm run demo-build && npm run lite-server",
    "demo-gh-pages": "npm run build && npm run demo-build && npm run demo-deploy",
    "demo-build": "ng build -prod --aot",
    "demo-deploy": "gh-pages -d demo/dist",
    "link": "ngm link -p src --here",
    "lint": "exit 0",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "build-report": "ngm build -p src --clean --prod --aot --sm",
    "build": "ngm build -p src --clean",
    "build-watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot",
    "commit": "git-cz",
    "build-package-link": "npm run build && npm run link",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "pretest": "npm run lint && npm run build-package-link",
    "test": "ng test -sr",
    "test-coverage": "ng test -sr -cc",
    "coveralls": "cat ./coverage/coverage.info | ./node_modules/.bin/coveralls && rm -rf coverage",
    "version": "npm run changelog && git add -A"
  },
  "peerDependencies": {
    "@angular/core": "^2.3.1 || >=4.0.0",
    "@angular/common": "^2.3.1 || >=4.0.0"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/compiler-cli": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/language-service": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@angular/router": "4.1.2",
    "@angular/tsc-wrapped": "4.1.2",
    "@types/jasmine": "2.5.47",
    "@types/marked": "0.0.28",
    "@types/node": "7.0.18",
    "@types/webpack": "2.2.15",
    "chokidar-cli": "1.2.0",
    "classlist-polyfill": "1.2.0",
    "codelyzer": "~3.0.1",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "1.3.1",
    "conventional-github-releaser": "1.1.3",
    "core-js": "^2.4.1",
    "coveralls": "^2.13.1",
    "cpy": "5.0.0",
    "cpy-cli": "1.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "del-cli": "1.0.0",
    "gh-pages": "1.0.0",
    "html-loader": "0.4.5",
    "jasmine": "2.6.0",
    "jasmine-core": "2.6.1",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "0.6.0",
    "karma-sauce-launcher": "1.1.0",
    "lite-server": "2.3.0",
    "lodash": "4.17.4",
    "ngm-cli": "0.5.0",
    "ngx-bootstrap": "1.6.6",
    "pre-commit": "1.2.2",
    "protractor": "5.1.1",
    "reflect-metadata": "0.1.10",
    "require-dir": "0.3.1",
    "rxjs": "5.4.0",
    "semantic-release": "^6.3.6",
    "systemjs-builder": "0.16.4",
    "ts-helpers": "^1.1.1",
    "ts-node": "3.0.4",
    "tslint": "5.2.0",
    "tslint-config-valorsoft": "2.0.1",
    "typedoc": "0.7.0",
    "typescript": "2.3.2",
    "wallaby-webpack": "0.0.38",
    "webdriver-manager": "12.0.6",
    "webpack-bundle-analyzer": "^2.8.1",
    "zone.js": "0.8.10"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=6.10.2"
  },
  "dependencies": {
    "feature-toggle-service": "^1.1.0"
  }
}
